    "vim.surround": true,
    "vim.autoindent": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["s"],
        "after": ["<leader>","<leader>", "s"]
        },
        {
        "before": ["<C-n>"],
        "commands": [":nohl"]
        },
    {
      "before": ["Z", "Z"],
      "commands": [":wq"]
    },
    {
      "before": ["Z", "Q"],
      "commands": [":q!"]
    },
    {
      "before": ["C-w"],
      "commands": [":tabclose"]
    },
    {
      "before": ["C-Space"],
      "commands": [":tabnext"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": [":tabprev"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-w>", "<C-l>"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "<C-h>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-w>", "<C-j>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-w>", "<C-k>"]
