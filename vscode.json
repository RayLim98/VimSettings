    /*
        Vim settings
    */
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.surround": true,
    "vim.autoindent": true,
    "vim.insertModeKeyBindings": [
        {
        "before": ["j", "k"],
        "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["s"],
        "after": ["<leader>", "<leader>", "s"]
        },
        {
        "before": ["<C-n>"],
        "commands": [":nohl"]
        },
        {
          "before": ["<leader>", "p"],
          "commands": [":tabprev"]
        },
        {
          "before": ["<C-l>"],
          "after": ["<C-w>", "<C-l>"]
        },
        {
          "before": ["<C-h>"],
          "after": ["<C-w>", "<C-h>"]
        },
        {
          "before": ["<C-j>"],
          "after": ["<C-w>", "<C-j>"]
        },
        {
          "before": ["<C-k>"],
          "after": ["<C-w>", "<C-k>"]
        },
        
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
